shader Parent
{
    float baseValue = 2.0f;
    Texture2D parentTexture;

    float AddBaseValue(float inValue)
    {
        float a0 = 0.0f,
              a1 = 1.0f;
        return inValue + baseValue + a0 + a1;
    }
};
