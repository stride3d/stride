shader TestExternArray
{
    compose ExternMixin externArray[];

    float test()
    {
        externArray[0].externFunc();
        externArray[1].externFunc();

        float a = externArray[0].externMember + externArray[1].externMember;
       
        foreach (var ext in externArray)
        {
            ext.externFunc();
            a += ext.externMember;
        }

        return a;
    }
};
