<Project>
	<Import Project="..\..\targets\Stride.Core.props" />
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<Description>Code generators for Stride.Core and its dependents</Description>
		<StrideSkipAutoPack>true</StrideSkipAutoPack>
		<!--<DefineConstants>LAUNCH_DEBUGGER;$(DefineConstants)</DefineConstants>-->
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\..\shared\SharedAssemblyInfo.cs">
			<Link>Properties\SharedAssemblyInfo.cs</Link>
		</Compile>
		<Compile Include="..\Stride.Core\Serialization\DataSerializerGenericMode.cs">
			<Link>Core\DataSerializerGenericMode.cs</Link>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.8.0" PrivateAssets="all" />
		<PackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="3.3.2" PrivateAssets="all" />
	</ItemGroup>
	<Import Project="$(StrideSdkTargets)" />
</Project>
