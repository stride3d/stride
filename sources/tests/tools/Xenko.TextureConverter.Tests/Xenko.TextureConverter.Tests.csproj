  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\tools\Xenko.TextureConverter\Xenko.TextureConverter.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Service Include="{82a7f48d-3b50-4b1e-b82e-3ada8210c358}" />
  </ItemGroup>
  <Import Project="$(XenkoPostSettingsTargets)" />
