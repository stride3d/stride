    <XenkoAssemblyProcessor>true</XenkoAssemblyProcessor>
    <TargetFramework>$(TargetFrameworkTool)</TargetFramework>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
