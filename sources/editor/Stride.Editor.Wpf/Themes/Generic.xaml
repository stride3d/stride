      <Setter.Value>
        <ControlTemplate TargetType="{x:Type view:StridePreviewView}">
          <ContentPresenter x:Name="PART_StrideView" />
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>
